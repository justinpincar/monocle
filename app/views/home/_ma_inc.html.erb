<script type="text/javascript">
  var _maq_accountId = '<%= accountId %>';

  <% if accountId == '4e868f5702d91a0217000004' and account_signed_in? %>
    var _maq_identify = {id: '<%= current_account.id %>', email: '<%= current_account.email %>'};
  <% else %>
  // Optional - You can provide us with the user's identity.
  //          - We will use this to merge user sessions, so be careful!
  // var _maq_identify = {id: userId, email: userEmail, name: userName};
 <% end %>

  var _maq = _maq || [];
  _maq.push(['_trackPageview']);
  (function() {
    var ma = document.createElement('script'); ma.type = 'text/javascript'; ma.async = true;
    ma.src = ('https:' == document.location.protocol ? 'https://' : 'http://') + 'sidesp.in/ma.js?accountId=' + _maq_accountId;
    var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ma, s);
  })();
</script>
